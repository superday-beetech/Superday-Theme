{
  "name": "Superday Light",
  "type": "light",
  "colors": {
    "editor.background": "#FFFFFF",
    "editor.foreground": "#000000"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#008000",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#A31515"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#0000FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": ["variable", "variable.other.php"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": "entity.name.class",
      "settings": {
        "foreground": "#795E26",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#0000FF"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#0000FF"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FF0000"
      }
    }
  ]
}
